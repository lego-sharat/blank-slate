{
  "manifest_version": 3,
  "name": "Minimal New Tab",
  "version": "1.0.0",
  "description": "A minimal, greyscale new tab page with todo list and notes",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage",
    "identity"
  ],
  "host_permissions": [
    "https://api.notion.com/*",
    "https://www.googleapis.com/*"
  ]
}
